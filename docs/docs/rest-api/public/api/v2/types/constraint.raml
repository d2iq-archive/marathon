#%RAML 1.0 Library
usage: |
  Defines Constraint types for new type definitions
types:
  ConstraintOperator:
    type: string
    enum: [UNIQUE, CLUSTER, GROUP_BY, LIKE, UNLIKE, MAX_PER]
  Constraint:
    type: object
    properties:
      fieldName: string
      operator: ConstraintOperator
      value?: string
  Constraints:
    type: array
    items: Constraint
