#%RAML 1.0 DataType
type: object
(scalaType): Constraint
description: |
  Valid constraint operators are one of UNIQUE, CLUSTER, GROUP_BY, LIKE, UNLIKE, MAX_PER
properties:
  fieldName:
    type: string
  operator:
    (scalaType): ConstraintOperator
    type: string
    enum: [UNIQUE, CLUSTER, GROUP_BY, LIKE, UNLIKE, MAX_PER]
  value?: string
