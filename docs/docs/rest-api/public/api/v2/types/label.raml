#%RAML 1.0 Library
types:
  Label:
    type: object
    properties:
      name:
        type: string
        minLength: 1
        maxLength: 255
        usage: |
          Users should avoid inventing labels prefixed with MARATHON_ MESOS_
          or DCOS_ since those may be defined by Marathon, Mesos, or DC/OS.
      value?:
        type: string
  Labels:
    type: array
    items: Label
